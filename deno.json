{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check --ignore=CHANGELOG.md && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable -A -",
    "manifest": "deno task cli manifest $(pwd)",
    "start": "deno run --unstable -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update .",
    "test": "deno test -A e2e/",
    "test:ci": "bash scripts/test-ci.sh"
  },
  "unstable": [
    "kv"
  ],
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ],
      "include": [
        "ban-untagged-todo",
        "camelcase",
        "eqeqeq",
        "explicit-function-return-type",
        "no-empty-interface",
        "no-explicit-any",
        "no-inferrable-types",
        "no-non-null-assertion",
        "no-throw-literal",
        "no-unused-vars",
        "prefer-const",
        "triple-slash-reference"
      ]
    },
    "exclude": [
      "./*.ts",
      "./*.tsx"
    ]
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "$std/": "https://deno.land/std@0.216.0/",
    "playwright": "npm:playwright@1.42.1",
    "@/": "./",
    "@routes/": "./routes/",
    "@islands/": "./islands/",
    "@components/": "./components/",
    "@serivces/": "./services/",
    "@data/": "./data/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
