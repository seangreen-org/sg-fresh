{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "test": "npx cypress run",
    "deploy": "deno run --allow-run ./scripts/deploy.ts"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "checkJs": true
  },
  "fmt": {
    "exclude": [
      "node_modules"
    ]
  },
  "lint": {
    "exclude": [
      "node_modules"
    ]
  }
}
